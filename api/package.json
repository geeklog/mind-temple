{
  "name": "mind-temple-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node src/app.ts",
    "copy-resource": "cp -rf src/views dist/views",
    "dev": "concurrently \"tsc -w\" \"nodemon\""
  },
  "nodemonConfig": {
    "watch": [
      "./src",
      "./dist"
    ],
    "ext": "ts",
    "events": {
      "restart": "cp -r src/views dist"
    }
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "image-size": "^0.9.3",
    "jade": "~1.11.0",
    "mikov": "^0.9.1",
    "morgan": "^1.9.1",
    "send": "^0.17.1",
    "sharp": "^0.26.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.14.5",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.0.5"
  }
}
